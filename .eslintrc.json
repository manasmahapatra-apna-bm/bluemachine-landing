{
    "extends": ["next/core-web-vitals"],
    "rules": {
        "no-console": "error",
        "no-debugger": "error",
        "indent": ["error", 4],
        "@typescript-eslint/no-unused-vars": [
            "error",
            {
                "argsIgnorePattern": "^_",
                "varsIgnorePattern": "^_"
            }
        ],
        "@typescript-eslint/explicit-function-return-type": "error",
        "@typescript-eslint/no-explicit-any": "error",
        "react-hooks/exhaustive-deps": "error"
    }
}
